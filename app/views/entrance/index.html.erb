    <div class="container">
      <div class="col-md-4 col-md-offset-1 col-sm-10 col-sm-offset-1" id="loginfield">
        <h2>ログイン</h2>
    
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <%= devise_error_messages! %>
    
          <div class="field">
            <%= f.email_field :email, autofocus: true, class: "form-control", placeholder: "email" %>
          </div>
    
          <div class="field">
            <%= f.password_field :password, autocomplete: "off", class: "form-control", placeholder: "パスワード" %>
          </div>
    
          <% if devise_mapping.rememberable? %>
            <div class="field">
              <%= f.check_box :remember_me %>
              <%= f.label :内容を記憶させる %>
            </div>
          <% end %>
    
          <div class="actions">
            <%= f.submit "ログイン", class: "signupbtn" %>
          </div>
        <% end %>
        <%= render "devise/shared/links" %>
        
      </div>
    </div>
    <div class="sec1">
      <h2 style="height:400px">Newslideの主な希望</h2>
    </div>
